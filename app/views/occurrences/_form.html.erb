<%= simple_form_for(@occurrence) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :data_begin %>
    <%= f.input :status %>
    <%= f.input :description %>
	<%= f.input :user_id %>
	<%= f.input :latitude %>
	<%= f.input :longitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
